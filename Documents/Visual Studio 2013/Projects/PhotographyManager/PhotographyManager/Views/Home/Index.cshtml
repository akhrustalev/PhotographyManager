@model PhotographyManager.DataAccess.Repositories.Users.UserRepository

@{
    ViewBag.Title = "Home Page";
}

<h3 style="color:white">Users' albums:</h3>
@using (Html.BeginForm())
{
    for (int i = 0; i < Model.GetAll().Count; i++) 
    { 
        <p>@Html.Label(Model.GetAll()[i].Name)</p>
        for (int j = 0; j < Model.GetAll()[i].Album.Count; j++) 
           { 
           @Html.ActionLink(Model.GetAll()[i].Album.ElementAt(j).Name + "     ", "ObservePhotos",new {ind1 = i, ind2 = j} ,null);
           }
         
    }        
}

 
